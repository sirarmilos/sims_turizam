<Window x:Class="InitialProject.View.Guest1Start"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View"
        mc:Ignorable="d"
        Title="Guest1Start" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock ="Top">
                <MenuItem Header="Search and show accommodations" Click="GoToSearchAndShowAccommodation"/>
            </Menu>
            <Grid DockPanel.Dock="Bottom" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="119*"/>
                    <ColumnDefinition Width="41*"/>
                </Grid.ColumnDefinitions>
            </Grid>
        </DockPanel>
    </Grid>
</Window>
